  <% include partials/header %>
    <h1><%= title %></h1>
    <% if (user) { %>
      Welcome back, <%= user.email %>
    <% } else { %>
      <p>Welcome to <%= title %></p>
    <% } %>
    <% swimming_holes.forEach(function(swimming_hole){
      <h2><%= swimming_hole.name %></h2>
      <a><img src="<%= swimming_hole.imageUrl %>" alt="Picture of <%= swimming_hole.name %>"></a>
    <% }) %>
  <% include partials/footer %>


