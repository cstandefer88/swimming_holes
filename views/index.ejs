  <% include partials/header %>
  <div class="hero">
    <div class="title">
      <h1 class="cursive"> SwimSpot </h1>
      <h1 class="notcursive"> Austin </h1>
    </div>
    <div class="arrow">
      <i class="fa fa-chevron-down fa-3x" aria-hidden="true"></i>
    </div>
  </div>
  <div>
    <h2 class="locations">Locations</h2>
  </div>
    <div>
      <% swimming_holes.forEach(function(swimming_hole){ %>
        <div class="swimmingHole">
          <h2 class="hole"><%= swimming_hole.name %></h2>
          <a href="/swimming_holes/<%= swimming_hole._id %>"><img src="<%= swimming_hole.imageUrl %>" alt="Picture of <%= swimming_hole.name %>"></a>
        </div>
      <% }) %>
    </div>
  <% include partials/footer %>
